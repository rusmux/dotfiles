    "*.tsx": "$(capture).ts, $(capture).*.ts, $(capture).*.tsx, $(capture)_*.ts, $(capture)_*.tsx, $(capture).css, $(capture).module.css, $(capture).less, $(capture).module.less, $(capture).module.less.d.ts, $(capture).scss, $(capture).module.scss, $(capture).module.scss.d.ts, $(capture).css.ts",
    "*.vbproj": "*.config, *proj.user, appsettings.*, bundleconfig.json",
    "*.vue": "$(capture).*.ts, $(capture).*.js, $(capture).story.vue",
    "*.w": "$(capture).*.w, I$(capture).w",
    "*.wat": "$(capture).wasm",
    "*.xaml": "$(capture).xaml.cs"
  },
  // Files
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.associations": {
    "Brewfile": "shellscript",
    "*.bats": "shellscript",
    "*.compose.yaml": "dockercompose",
    "*ignore": "ignore"
  },
  "files.exclude": {
    "**/.idea": true,
    "**/.exercism": true,
    "**/target": true,
    "**/.build": true,
    // Python
    "**/__pycache__": true,
    "**/.ipynb_checkpoints": true,
    "**/.virtual_documents": true,
    "**/.ruff_cache": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/*.coverage*": true
  },
  "files.watcherExclude": {
    "**/.idea": true,
    "**/.exercism": true,
    "**/.target": true,
    "**/.build": true,
    "**/node_modules": true,
    // Python
    "**/.venv": true,
    "**/__pycache__": true,
    "**/.ipynb_checkpoints": true,
    "**/.virtual_documents": true,
    "**/.ruff_cache": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/*.coverage*": true
  },
  // Search
  "search.exclude": {
    "**/node_modules": true,
    "**/.venv": true
  },
  "search.mode": "newEditor",
  "search.defaultViewMode": "tree",
  "search.searchEditor.defaultNumberOfContextLines": 3,
  // Editor
  "breadcrumbs.enabled": false,
  "editor.fontSize": 13,
  "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
  "editor.minimap.enabled": false,
  "editor.accessibilitySupport": "off",
  "editor.tabCompletion": "on",
  // "editor.rulers": [120],
  "editor.lineNumbers": "off",
  "editor.glyphMargin": false,
  "editor.guides.indentation": false,
  "editor.scrollbar.verticalScrollbarSize": 7,
  "editor.scrollbar.vertical": "hidden",
  "editor.quickSuggestionsDelay": 200,
  "diffEditor.ignoreTrimWhitespace": false,
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-containers"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "docker.docker"
  },
  // Terminal
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.scrollback": 2000,
  "terminal.integrated.stickyScroll.enabled": true,
  "terminal.integrated.tabs.title": "${cwdFolder}",
  "terminal.integrated.tabs.description": "${process}${separator}${task}${separator}${local}",
  "terminal.integrated.env.osx": {
    "Q_NEW_SESSION": "1"
  },
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.enableImages": true,
  "terminal.external.osxExec": "/Applications/My/Programming/WezTerm.app",
  "terminal.integrated.minimumContrastRatio": 2,
  // Other
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "off",
  "redhat.telemetry.enabled": false,
  "chat.commandCenter.enabled": false,
  "yaml.format.printWidth": 120,
  "yaml.format.singleQuote": true,
  "yaml.schemas": {
    "https://raw.githubusercontent.com/canonical/cloud-init/main/cloudinit/config/schemas/versions.schema.cloud-config.json": [
      "**/*user-data"
    ]
  },
  "inlineChat.enableV2": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "remote.autoForwardPorts": false,
  // Theme
  "catppuccin.italicKeywords": false,
  "catppuccin.italicComments": false,
  "catppuccin.accentColor": "",
  "catppuccin.extraBordersEnabled": true,
  "catppuccin.workbenchMode": "flat",
  "catppuccin.colorOverrides": {
    "latte": {
      "base": "#F6F6F6",
      "mantle": "#EAEAEA"
    }
  },
  // ? TOOLS
  // Git
  "git.openRepositoryInParentFolders": "never",
  "git.untrackedChanges": "separate",
  "git.autofetch": true,
  "git.autofetchPeriod": 300,
  "github.copilot.enable": {
